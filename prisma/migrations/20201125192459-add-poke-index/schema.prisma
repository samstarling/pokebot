datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Pokemon {
  number Int @id
  generation Int
  name String

  @@index([generation])
}

model Roll {
  id Int @default(autoincrement()) @id
  createdAt DateTime @default(now())
  teamId String
  userId String
  pokemonNumber Int

  Pokemon Pokemon @relation(fields: [pokemonNumber], references: [number])
}
